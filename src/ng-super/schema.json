{
	"$schema": "http://json-schema.org/schema",
	"id": "megaModule",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"$default": {
				"$source": "argv",
				"index": 0
			},
			"description": "Mega Module Name",
			"x-prompt": "What is the mega entity name?"
		},
		"selector": {
			"type": "string",
			"format": "html-selector",
			"description": "The HTML selector to use for this component."
		}
	},
	"required": [
		"name"
	]
}